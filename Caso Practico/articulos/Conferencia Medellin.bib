@article{Mo2016,
abstract = {{\textcopyright} 2016 IEEE. We consider the problem of estimating the state of a linear time-invariant Gaussian system using m sensors, where a subset of the sensors can potentially be compromised by an adversary. We prove that under mild assumptions, we can decompose the optimal Kalman estimate as a weighted sum of local state estimates, each of which is derived using only the measurements from a single sensor. We then propose a convex optimization based approach, instead of the weighted sum approach, to combine the local estimate into a more secure state estimate. Our proposed estimator coincides with the Kalman estimator with certain probability when all sensors are benign and is stable when less than half of the sensors are compromised. Numerical simulations are provided to illustrate the performance of the proposed state estimation scheme.},
author = {Mo, Yilin and Garone, Emanuele},
doi = {10.1109/CDC.2016.7799045},
file = {:D$\backslash$:/Doctorado/Quinto Semestre/Art. 2019/07799045.pdf:pdf},
isbn = {9781509018376},
journal = {2016 IEEE 55th Conference on Decision and Control, CDC 2016},
number = {Cdc},
pages = {5073--5078},
publisher = {IEEE},
title = {{Secure dynamic state estimation via local estimators}},
year = {2016}
}
@article{Albinali2018,
abstract = {{\textcopyright} 2017 IEEE. Power systems around the world are experiencing an evolution driven by the introduction of new energy resources. This evolution introduces new challenges one of these challenges is the risk of power system interruptions caused by relay misoperations. Technological advancement enables proposing new approaches to secure the protection system from such misoperations. This paper introduces new substation centralized protection scheme which monitors the protection system within the substation to detect hidden failures and increase protection system security. This monitoring is achieved by performing dynamic state estimation in real time. The process started by data streaming from the IEDs within the susbation to the CPS. Then, the CPS processes these data and performs the dynamic sate estimation. The CPS uses the Chi-square test to detect Abnormality. Upon detecting abnormality, hypothesis test is performed to classify the problem to either hidden failures or power faults. The detection of hidden failures initiate blocking signal to secure the affected IED from misoperations. The paper introduces the proposed architecture to provide a reliable data management and optimize the required communication system within the substation.},
author = {Albinali, H. F. and Meliopoulos, A. P.},
doi = {10.1109/SASG.2017.8356476},
file = {:D$\backslash$:/Doctorado/Quinto Semestre/Art. 2019/08356476.pdf:pdf},
isbn = {9781538618769},
journal = {2017 Saudi Arabia Smart Grid Conference, SASG 2017},
keywords = {Architecture,Centralized protection scheme,Dynamic state estimation,Hidden failures,IED},
pages = {1--6},
title = {{Hidden failure detection via dynamic state estimation in substation protection systems}},
year = {2018}
}
@book{Blanke2011,
abstract = {This book reports the development of fault diagnosis and fault-tolerant control (FTC) methods with their application to real plants. After an introduction to fault diagnosis and FTC, a chapter on actuators and sensors in systems with varying degrees of nonlinearity leads to three chapters in which the design of FTC systems is given thorough coverage for real applications: a winding machine typifying a subsystem in various sheet and film processes; a hydraulic 3-tank system representative of those used widely in chemical plants; and an active suspension system demonstrating application in whole large-scale systems by splitting into subsystems. Actuator and sensor faults are accommodated within the control-law design and the integration of fault diagnosis models in the FTC systems described. Linearized systems around an operating point and nonlinear systems are discussed and illustrated. A complete simulation platform of the 3-tank system is provided via download from www.springer.com},
author = {Noura, H and Didier, T and Jean-Christophe, P and Abbas, C.},
booktitle = {Advances in Control},
doi = {10.1007/978-1-4471-0853-5_6},
editor = {Springer},
file = {:D$\backslash$:/Doctorado/Cuarto Semestre/Tesis I/FaultToleantSysByExample.pdf:pdf},
isbn = {9781848826526},
pages = {171--196},
title = {{Fault-tolerant Control Systems, Design and Practical Applications}},
year = {2011}
}
@article{Rana2017,
abstract = {The penetration of renewable distributed energy resources such as wind turbine has been dramatically integrated in distribution networks due to clean, sustainable, and economical green energy. Due to intermittent wind speed, the power generation patterns vary which can risk the distribution network operation. So, it is intrinsically required to monitor the wind turbines in a distributed way. This paper presents an adaptive-then-combine distributed dynamic approach for monitoring the grid under lossy communication link between the wind turbines and energy management system. Firstly, the wind turbine is represented by a state-space linear equation, with sensors deployed to obtain system state information. Based on the mean squared error principle, an adaptive approach is proposed to estimate the local state information. The global estimation is designed by combining estimation results with weighting factors which are calculated by minimizing estimation error covariance based on semi-definite programming. Finally, the convergence analysis indicates that estimation error is gradually decreased, so the estimated state converges to the actual state. The efficacy of the developed approach is verified using the turbine model. The research is valuable for green energy technologies, households and interconnected industrial information societies, and also provides the knowledge towards a smart and secure green energy future.},
author = {Rana, Md Masud and Li, Li and Su, Steven W.},
doi = {10.1109/ICARCV.2016.7838784},
file = {:D$\backslash$:/Doctorado/Quinto Semestre/Art. 2019/07838784.pdf:pdf},
isbn = {9781509035496},
journal = {2016 14th International Conference on Control, Automation, Robotics and Vision, ICARCV 2016},
keywords = {Adaptive-then-combine,Kalman filter,Lyapunov function,packet losses,semidefinite programming,wind turbine},
number = {November 2016},
pages = {13--15},
title = {{Distributed dynamic state estimation considering renewable generation and packet losses}},
volume = {2016},
year = {2017}
}
@article{Hu2017,
abstract = {This paper focuses on securely estimating the state of a nonlinear dynamical system from a set of corrupted measurements. In particular, we consider two broad classes of nonlinear systems, and propose a technique which enables us to perform secure state estimation for such nonlinear systems. We then provide guarantees on the achievable state estimation error against arbitrary corruptions, and analytically characterize the number of errors that can be perfectly corrected by a decoder. To illustrate how the proposed nonlinear estimation approach can be applied to practical systems, we focus on secure estimation for the wide area control of an interconnected power system under cyber-physical attacks and communication failures, and propose a secure estimator for the power system. Finally, we numerically show that the proposed secure estimation algorithm enables us to reconstruct the attack signals accurately.},
author = {Hu, Qie and Fooladivanda, Dariush and Chang, Young Hwan and Tomlin, Claire J.},
doi = {10.23919/ACC.2017.7963372},
file = {:D$\backslash$:/Doctorado/Quinto Semestre/Art. 2019/07963372.pdf:pdf},
isbn = {9781509059928},
issn = {07431619},
journal = {Proceedings of the American Control Conference},
pages = {2779--2784},
publisher = {AACC},
title = {{Secure state estimation for nonlinear power systems under cyber attacks}},
year = {2017}
}
@article{Wang2018,
abstract = {With the popularity of Supervisory Information System (SIS), Supervisory Control and Data Acquisition (SCADA) system and Internet of Things (IoT) sensors, we can easily obtain abundant sensor data in manufacturing. We could save manufacturing maintenance costs and prevent further damages if we can accurately predict system anomalies from the sensor data. Yet learning from individual sensors often cannot directly determine whether the system will have anomaly because each sensor only measures a partial state of a big system. By detecting events across sensors collectively and their temporal dependencies, this paper proposes a new system-level anomaly prediction framework by mining anomaly dependency graph from sensor data. The advantages of the approach include explainability, collective prediction and temporal sensitivity. We applied our approach with a real-world power plant dataset to evaluate its feasibility.},
author = {Wang, Jianwu and Liu, Chen and Zhu, Meiling and Guo, Pei and Hu, Yapeng},
doi = {10.1109/BigDataCongress.2018.00028},
file = {:D$\backslash$:/Doctorado/Quinto Semestre/Art. 2019/08457744.pdf:pdf},
isbn = {9781538672327},
journal = {Proceedings - 2018 IEEE International Congress on Big Data, BigData Congress 2018 - Part of the 2018 IEEE World Congress on Services},
keywords = {Anomaly Prediction,Data Stream Mining,Predictive Maintenance,Sensor Data Driven,Smart Manufacturing},
pages = {158--165},
publisher = {IEEE},
title = {{Sensor Data Based System-Level Anomaly Prediction for Smart Manufacturing}},
year = {2018}
}
@article{Pasqualetti2013a,
abstract = {Cyber-physical systems are ubiquitous in power systems, transportation networks, industrial control processes, and critical infrastructures. These systems need to operate reliably in the face of unforeseen failures and external malicious attacks. In this paper: (i) we propose a mathematical framework for cyber-physical systems, attacks, and monitors; (ii) we characterize fundamental monitoring limitations from system-theoretic and graph-theoretic perspectives; and (ii) we design centralized and distributed attack detection and identification monitors. Finally, we validate our findings through compelling examples.},
