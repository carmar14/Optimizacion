issn = {23255870},
journal = {IEEE Transactions on Control of Network Systems},
keywords = {Secure cyber-physical systems,secure state reconstruction,sensor attacks},
number = {1},
pages = {49--59},
publisher = {IEEE},
title = {{Secure state estimation against sensor attacks in the presence of noise}},
volume = {4},
year = {2017}
}
